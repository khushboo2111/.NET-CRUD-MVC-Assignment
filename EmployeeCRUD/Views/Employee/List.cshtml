
@model List<Employee>
<h2>Employees Details</h2>


<table class = "table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Department</th>
            <th>Project</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Department</td>
                <td>@employee.Project</td>
                <td>@employee.Age</td>
                <td>
                    <a class="btn btn-primary" asp-controller="Employee" asp-action="Edit" asp-route-id="@employee.Id">Edit</a>
                </td>

            </tr>
        }
    </tbody>
</table>